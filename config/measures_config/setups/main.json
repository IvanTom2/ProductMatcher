{
  "config_name": "main",
  "numeric_measures": {
    "use_it": true,
    "measures": [
      {
        "measure_name": "Объем",
        "autosem": {
          "merge_mode": "1",
          "exclude_rx": false,
          "use_it": true
        },
        "feature_validator": {
          "use_it": true,
          "validation_mode": "strict",
          "not_found_mode": "strict",
          "priority": 10
        },
        "measure_data": {
          "common_prefix": "[^\\\\\\/а-яa-z0-9]",
          "common_postfix": "",
          "common_max_count": 1,
          "special_value_search": null,
          "features": [
            {
              "feature_name": "Миллилитр",
              "defenition": "мл|миллилитр(?:ов)?|ml|milliliters?|millilitres?",
              "relative_weight": 1,
              "prefix": "common",
              "postfix": "common",
              "max_count": 1,
              "search_mode": "behind",
              "use_it": true
            },
            {
              "feature_name": "Литр",
              "defenition": "литр(?:ов)?|л|l|litres?|liters?",
              "relative_weight": 1000,
              "prefix": "common",
              "postfix": "[^а-яa-z]",
              "max_count": 1,
              "search_mode": "behind",
              "use_it": true
            }
          ]
        }
      },
      {
        "measure_name": "Вес",
        "autosem": {
          "merge_mode": "1",
          "exclude_rx": false,
          "use_it": true
        },
        "feature_validator": {
          "use_it": true,
          "validation_mode": "strict",
          "not_found_mode": "strict",
          "priority": 10
        },
        "measure_data": {
          "common_prefix": "[^0-9]",
          "common_postfix": "[^\\\\/]",
          "common_max_count": 1,
          "special_value_search": null,
          "features": [
            {
              "feature_name": "Микрограмм",
              "defenition": "мкг|микрограмм|µg|ug|microgram",
              "relative_weight": 1,
              "prefix": "common",
              "postfix": "[^\\\\/a-z]",
              "max_count": 1,
              "search_mode": "behind",
              "use_it": true
            },
            {
              "feature_name": "Миллиграмм",
              "defenition": "мг|миллиграмм|mg|milligram",
              "relative_weight": 1000,
              "prefix": "common",
              "postfix": "common",
              "max_count": 1,
              "search_mode": "behind",
              "use_it": true
            },
            {
              "feature_name": "Грамм",
              "defenition": "г|гр|г[.]|грамм|g|gr|gram",
              "relative_weight": 1000000,
              "prefix": "common",
              "postfix": "[^\\\\/a-zа-я]",
              "max_count": 1,
              "search_mode": "behind",
              "use_it": true
            },
            {
              "feature_name": "Килограмм",
              "defenition": "кг|килограмм|kg|kilogram",
              "relative_weight": 1000000000,
              "prefix": "common",
              "postfix": "common",
              "max_count": 1,
              "search_mode": "behind",
              "use_it": true
            }
          ]
        }
      },
      {
        "measure_name": "Количество",
        "autosem": {
          "merge_mode": "1",
          "exclude_rx": true,
          "use_it": true
        },
        "feature_validator": {
          "use_it": true,
          "validation_mode": "strict",
          "not_found_mode": "strict",
          "priority": 10
        },
        "measure_data": {
          "common_prefix": "[^0-9]",
          "common_postfix": "[^а-я]",
          "common_max_count": 1,
          "special_value_search": "([2-9][,.]?\\d*|[1-9]\\d+[,.]?\\d*)",
          "features": [
            {
              "feature_name": "Штуки",
              "defenition": "шт|уп|пач|штук|упак|упаковок|пачек",
              "relative_weight": 1,
              "prefix": "common",
              "postfix": "common",
              "max_count": 1,
